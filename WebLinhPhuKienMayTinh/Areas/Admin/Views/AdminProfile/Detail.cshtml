@model WebLinhPhuKienMayTinh.Models.EF.ADMIN
@{
    ViewBag.Title = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_Layouts.cshtml";
}
<div class="box box-default">
    <div class="box-header with-border">
        <h3 class="box-title">Hồ sơ</h3>

    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <div class="box box-primary">
            <div class="box-body box-profile">
                <img class="profile-user-img img-responsive img-circle" src="/Assets/Admin/dist/img/26219765_153092935338641_7804066461055097235_n.jpg" alt="User profile picture">

                <h3 class="profile-username text-center">@Model.adminName</h3>

                <ul class="list-group list-group-unbordered" style="margin-left: 30%;margin-right: 30%;">
                    <li class="list-group-item">
                        <b>Tên </b> <a class="pull-right">@Model.adminName</a>
                    </li>
                    <li class="list-group-item">
                        <b>Email</b> <a class="pull-right">@Model.adminEmail</a>
                    </li>
                    <li class="list-group-item">
                        <b>Level</b><a class="pull-right">@Model.level0</a>
                    </li>

                </ul>


            </div>
            <!-- /.box-body -->
        </div>
    </div>
</div>

